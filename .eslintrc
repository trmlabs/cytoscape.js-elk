{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "amd": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "prefer-const": "error",
    "no-var": "error"
  }
}
